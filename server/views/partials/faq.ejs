<!--FAQ-->
<section class="text-center" ng-controller="FaqCtrl">
  <h1>FAQ</h1>
  <div class="row">
    <div class="small-12 column" ng-repeat="panel in panels">
      <div class="panel-title" toggle-sibling>
        {{panel.question}}
      </div>
      <div class="panel" hidden>
        <p>
          {{panel.answer}}
        </p>
      </div>
    </div>
  </div>
</section>
