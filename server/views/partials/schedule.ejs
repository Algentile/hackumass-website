<!--Schedule-->
<section class="text-center alternate schedule" ng-controller="ScheduleCtrl">
  <h2>{{title}}</h2>

  <div class="row">
    <div class="small-12 column" ng-repeat="day in schedule">
      <h4>{{day.day}}</h4>
      <table>
        <tr ng-repeat="event in day.events">
          <td>{{event.time}}</td>
          <td>{{event.action}}</td>
        </tr>
      </table>
    </div>
  </div>

</section>
