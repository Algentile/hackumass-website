<!--Schedule-->
<section class="text-center alternate" ng-controller="ScheduleCtrl">
  <h1>{{title}}</h1>

  <div class="row">
    <div class="small-12 medium-6 column" ng-repeat="day in schedule">
      <h3>{{day.day}}</h3>
      <p ng-repeat="event in day.events">
        {{event.time}} - {{event.action}}
      </p>
    </div>
  </div>

</section>
